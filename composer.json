{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "anlutro/l4-settings": "^1.0",
        "doctrine/dbal": "^2.11",
        "fabpot/goutte": "^4.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "jeremeamia/slack-block-kit": "^0.8.3",
        "laravel/framework": "^8.7",
        "laravel/horizon": "^5.1",
        "laravel/slack-notification-channel": "^2.2",
        "laravel/tinker": "^2.0",
        "php-http/curl-client": "^1.0",
        "predis/predis": "^1.1",
        "sentry/sentry-laravel": "^1.9",
        "spatie/laravel-event-sourcing": "^4.5",
        "spatie/laravel-failed-job-monitor": "^3.3",
        "spatie/laravel-queueable-action": "^2.6",
        "spatie/laravel-webhook-client": "^2.7",
        "ylsideas/feature-flags": "^1.3"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
